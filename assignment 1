#include<iostream>
using namespace std;
int main()
{
    int a[7];
    for (int i=0; i<7; i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<7;i++)
    {
        if(a[i]==0)
        {
            cout<<a[i]<<" is zero"<<endl;
        }
        if(a[i]>0)
        {
            cout<<a[i]<<" is positive"<<endl;
        }
        if(a[i]<0)
        {
            cout<<a[i]<<" is negative"<<endl;
        }
    }
    return 0;
}
